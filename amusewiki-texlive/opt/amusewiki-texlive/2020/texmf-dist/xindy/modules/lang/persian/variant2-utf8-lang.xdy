(require "lang/persian/variant2-utf8.xdy")
(define-sort-rule-orientations (forward backward forward forward))
(use-rule-set :run 0
	      :rule-set ("pe-alphabetize" "pe-ignore-special"))
(use-rule-set :run 1
	      :rule-set ("pe-resolve-diacritics" "pe-ignore-special"))
(use-rule-set :run 2
	      :rule-set ("pe-resolve-case" "pe-ignore-special"))
(use-rule-set :run 3
	      :rule-set ("pe-resolve-special"))
